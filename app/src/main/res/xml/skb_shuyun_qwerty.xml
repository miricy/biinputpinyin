<?xml version="1.0" encoding="utf-8"?>

<keyboard
    skb_template="@xml/skb_shuyun_template"
    skb_cache_flag="true"
    qwerty="true"
    qwerty_uppercase="true"
    width="5%p"
    height="20%p"
    key_type="0"
    repeat="false"
    balloon="false">

    <row start_pos_x="16%p" start_pos_y="6%p">
        <key label="" code="102" width="9%p" icon="@drawable/ic_key_l1" start_pos_x="1%p"/>
